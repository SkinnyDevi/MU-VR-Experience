{
  "httpPort": 5488,
  "store": {
    "provider": "fs"
  },
  "blobStorage": {
    "provider": "fs"
  },
  "logger": {
    "console": {
      "transport": "console",
      "level": "debug"
    },
    "file": {
      "transport": "file",
      "level": "info",
      "filename": "jsreport/logs/reporter.log"
    },
    "error": {
      "transport": "file",
      "level": "error",
      "filename": "jsreport/logs/error.log"
    }
  },
  "allowLocalFilesAccess": true,
  "reportTimeout": 60000,
  "workers": {
    "numberOfWorkers": 2
  },
  "extensions": {
    "authentication": {
      "cookieSession": {},
      "admin": {
        "username": "admin",
        "password": "password"
      },
      "enabled": false
    },
    "fs-store": {
      "dataDirectory": "./jsreport"
    },
    "sample-template": {
      "createSamples": true
    }
  },
  "sandbox": { 
		"allowedModules": ["https", "mysql2"] 
	}
}
